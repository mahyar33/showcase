{
  "name": "web-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "axios": "^0.18.0",
    "connected-react-router": "^6.3.2",
    "cross-env": "^5.2.0",
    "flow-bin": "^0.97.0",
    "fontfaceobserver": "^2.1.0",
    "history": "^4.9.0",
    "hoist-non-react-statics": "^3.3.0",
    "immutable": "^4.0.0-rc.12",
    "intl": "^1.2.5",
    "invariant": "^2.2.4",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.23",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.8.0",
    "react-redux": "^7.0.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
    "redux-immutable": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-persist-transform-immutable": "^5.0.0",
    "redux-saga": "^1.0.2",
    "reselect": "^4.0.0",
    "socket.io-client": "^2.2.0",
    "styled-components": "^4.2.0"
  },
  "scripts": {
    "start": "cross-env npm-run-all -p start-js watch-langs",
    "github-pages": "npm run doc && gh-pages-deploy",
    "doc-windows": "del /f /s /q \"./docs\" && docco {src,test}/{**/*,*} README.md misc/tutorials/{**/*,*} -t ./misc/template/docco.jst -c ./misc/template/docco.css && xcopy /S /E \"./misc/template/public\" \"./docs\"",
    "doc": "rm -fr ./docs && docco {src,test}/{**/**/*,**/*,*} README.md misc/tutorials/{**/**/*,**/*,*} -t ./misc/template/docco.jst -c ./misc/template/docco.css && cp -r ./misc/template/public ./docs",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch-langs": "cross-env NODE_ENV=development nodemon --watch src/**/en.json --watch src/**/fa.json  --exec npm run build-langs",
    "build-langs": "node_modules/.bin/babel-node src/utils/CompoundIntl.js",
    "start-js": "react-scripts start",
    "flow": "flow"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "gh-pages-deploy": {
    "staticpath": "docs"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "docco": "^0.8.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "gh-pages-deploy": "^0.5.1",
    "reactotron-react-js": "^3.0.2",
    "reactotron-redux": "^3.1.0",
    "reactotron-redux-saga": "^4.2.1",
    "sinon": "^7.3.1"
  }
}
